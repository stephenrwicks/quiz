{
  "compileOnSave": true,
  "compilerOptions": {
    "removeComments": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2020"
    ],
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "outDir": ""
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "**/*.js"
  ]
}